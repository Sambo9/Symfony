{% extends "::base.html.twig" %}

{% block title %}CAPlatypusBundle:CheckerController:showParams{% endblock %}

{% block body %}

<p> Array length: {{tab|length}} </p>

<table class="table table-bordered">
	<tr>
		<th>Name</th>
		<th>value</th>
	</tr>
	
		{% for key, value in tab %}
